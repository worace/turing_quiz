<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @quiz.title %>
</p>

<p>
  <strong>Slug:</strong>
  <%= @quiz.slug %>
</p>

<% @quiz.questions.each do |q| %>
  <p><strong>Question: </strong><%= q.body %></p>
<% end %>

<%= link_to 'Edit', edit_quiz_path(@quiz) %> |
<%= link_to 'Add Question', new_quiz_question_path(@quiz) %> |
<%= link_to 'Back', quizzes_path %>
